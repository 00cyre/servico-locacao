<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
	<changeSet id="cria_tabela_rental" author="bertod777@gmail.com">
		<createTable tableName="Rental">
			<column name="Id" type="int" autoIncrement="true">
				<constraints primaryKey="true" nullable="false" />
			</column>
			<column name="MovieId" type="int">
				<constraints references="movies(Id)" foreignKeyName="fk_MovieId" nullable="false" />
			</column>
			<column name="UserId" type="int">
				<constraints references="users(Id)" foreignKeyName="fk_UserId" />
			</column>
			<column name="DataDevolucao" type="DATE" />
		</createTable>
	</changeSet>
	<changeSet id="tag-1.2" author="bertod777@gmail.com">
		<tagDatabase tag="1.2" />
	</changeSet>
</databaseChangeLog>
